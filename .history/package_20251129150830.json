{
  "name": "event-ticket-booking-system",
  "version": "1.0.0",
  "description": "A Node.js RESTful API for event ticket booking with concurrency handling",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/unit",
    "test:integration": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/integration",
    "db:migrate": "sequelize db:migrate --config config/config.cjs",
    "db:create": "sequelize db:create --config config/config.cjs",
    "db:migrate:undo": "sequelize db:migrate:undo"
  },
  "keywords": [
    "ticket",
    "booking",
    "nodejs",
    "express",
    "api",
    "concurrency"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "async-lock": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
